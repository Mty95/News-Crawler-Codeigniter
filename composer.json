{
  "config": {
	"vendor-dir": "vendor"
  },
  "autoload": {
	"psr-4": {
	  "": "./application/third_party",
	  "NewFramework\\": "./application/third_party/Mty95/NewFramework",
	  "Core\\": "./application/core",
	  "App\\Helper\\": "./application/helpers",
	  "App\\Library\\": "./application/libraries",
	  "App\\Model\\": "./application/models",
	  "App\\Exceptions\\": "./application/exceptions",
	  "App\\Console\\": "./application/console",
	  "App\\Console\\Commands\\": "./application/console/commands",
	  "Tests\\": "tests"
	},
	"files": [
	  "./application/Common.php",
	  "./application/Services.php",
	  "./application/Repositories.php"
	],
	"classmap": []
  },
  "require": {
	"php": ">=7.2",
	"ext-json": "*",
	"ext-openssl": "*",
	"ext-intl": "*",
	"fzaninotto/faker": "*",
	"chriskacerguis/codeigniter-restserver": "3.0.3",
	"firebase/php-jwt": "^5.0",
	"symfony/dom-crawler": "^4.4",
	"symfony/css-selector": "^4.4",
	"symfony/browser-kit": "^4.4",
	"symfony/http-client": "^4.4"
  },
  "require-dev": {
	"phpunit/phpunit": "8"
  },
  "replace": {
	"codeigniter/framework": "*"
  }
}
